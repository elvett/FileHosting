{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "_comment": "Stryker configuration for a Vitest project",
  "testRunner": "vitest",
  "reporters": [
    "progress",
    "clear-text",
    "html"
  ],
  "coverageAnalysis": "off",
  "mutate": [
    "app/**/*.ts"
  ],
  "vitest": {
    "configFile": "vitest.config.mts"
  }
}
